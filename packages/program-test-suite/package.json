{
  "name": "@saasquatch/program-test-suite",
  "version": "2.6.1-0",
  "description": "A collection of utilities for creating program unit tests",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "tsc -w",
    "prepublishOnly": "if [ \"$CI\" != \"true\" ]; then echo Error: Publishing manually is not supported. Please use the GitHub action. && exit 1; fi",
    "prepack": "npm run build",
    "build": "tsc"
  },
  "prettier": {
    "trailingComma": "es5",
    "tabWidth": 2,
    "semi": true,
    "singleQuote": false,
    "printWidth": 80
  },
  "files": [
    "dist"
  ],
  "main": "dist/index.js",
  "license": "MIT",
  "author": "ReferralSaaSquatch.com, Inc.",
  "repository": {
    "url": "https://github.com/saasquatch/program-tools"
  },
  "publishConfig": {
    "provenance": true
  },
  "engines": {
    "node": "^18 || ^20 || ^22"
  },
  "dependencies": {
    "@saasquatch/program-boilerplate": "^3.10.0",
    "deepmerge": "^4.2.2",
    "dlv": "^1.1.3",
    "faker": "^5.3.1",
    "glob": "^7.1.6",
    "jest": "^26.6.3",
    "jest-cucumber": "^3.0.0",
    "ts-jest": "^26.5.0"
  },
  "devDependencies": {
    "@types/dlv": "^1.1.2",
    "@types/express": "^4.17.11",
    "@types/faker": "^5.1.6",
    "@types/node": "^14.14.22",
    "typescript": "^5.5.2"
  }
}

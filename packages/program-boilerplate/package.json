{
  "name": "@saasquatch/program-boilerplate",
  "version": "3.5.12-0",
  "description": "Boilerplate for writing programs",
  "main": "dist/index.js",
  "files": [
    "dist/"
  ],
  "scripts": {
    "build": "tsc --declaration",
    "build:docs": "typedoc --out ./docs ./src --ignoreCompilerErrors --media assets --hideGenerator",
    "format": "prettier \"src/**/*.js\" --write",
    "prepush": "npm run format",
    "prepare": "npm run build",
    "deploy": "np",
    "test": "jest"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/express": "^4.17.11",
    "@types/node": "^13.13.50",
    "@types/supertest": "^2.0.11",
    "jest": "^26.6.3",
    "jest-cucumber": "^3.0.1",
    "np": "^6.5.0",
    "prettier": "^2.2.1",
    "supertest": "^6.1.3",
    "ts-jest": "^26.5.5",
    "ts-node": "^9.1.1",
    "typedoc": "^0.17.8",
    "typescript": "^3.9.9"
  },
  "dependencies": {
    "bson-objectid": "^1.3.1",
    "compression": "^1.7.4",
    "express": "^4.17.1",
    "jsonata": "^1.8.4",
    "winston": "^3.3.3"
  }
}

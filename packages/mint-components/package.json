{
  "name": "@saasquatch/mint-components",
  "version": "1.1.1",
  "description": "Mint Components - Built with Shoelace Components",
  "main": "dist/index.cjs.js",
  "es2015": "dist/esm/index.js",
  "es2017": "dist/esm/index.js",
  "types": "dist/types/components.d.ts",
  "module": "dist/index.js",
  "jsnext:main": "dist/esm/index.js",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "files": [
    "dist/",
    "grapesjs/",
    "shoelace/"
  ],
  "scripts": {
    "build": "stencil build --docs",
    "build:grapes": "cross-env BUILD=DOCS stencil build --watch --docs",
    "serve:grapes": "npx http-server . --cors -c-1 -p 8080",
    "start": "cross-env NODE_ENV=dev stencil build --dev --watch --serve",
    "test": "stencil test --spec --e2e",
    "test.watch": "stencil test --spec --e2e --watchAll",
    "generate": "stencil generate",
    "specs": "picklesdoc xlsx src docs --testers=2"
  },
  "devDependencies": {
    "@formatjs/intl": "1.8.4",
    "@formatjs/intl-pluralrules": "^1.5.8",
    "@formatjs/intl-relativetimeformat": "4.5.15",
    "@formatjs/intl-utils": "^2.2.5",
    "@lit/reactive-element": "^1.0.0-rc.2",
    "@rollup/plugin-alias": "^3.1.2",
    "@stencil/core": "2.4.0",
    "@stencil/router": "^1.0.1",
    "@stencil/sass": "^1.3.2",
    "@stencil/utils": "latest",
    "@types/jest": "^26.0.21",
    "@types/puppeteer": "^5.4.2",
    "@types/resize-observer-browser": "^0.1.5",
    "clipboard": "^1.7.1",
    "cross-env": "^7.0.2",
    "docx": "^5.4.1",
    "dom-context": "^1.2.0",
    "jest": "^26.6.3",
    "jest-cli": "^26.6.3",
    "jest-config": "^26.6.3",
    "rollup-plugin-copy": "^3.4.0",
    "rollup-plugin-css-only": "^3.1.0",
    "rollup-plugin-string": "^3.0.0",
    "stencil-docx-docs": "0.0.2",
    "ts-node": "^7.0.1",
    "workbox-build": "4.3.1"
  },
  "dependencies": {
    "@saasquatch/component-boilerplate": "^1.2.1",
    "@saasquatch/stencil-grapes-plugin": "^1.0.1",
    "@saasquatch/stencil-hooks": "^2.0.1-0",
    "@saasquatch/stencilbook": "^1.0.0",
    "@saasquatch/universal-hooks": "^1.0.0",
    "@shoelace-style/shoelace": "^2.0.0-beta.32",
    "@stencil/helmet": "^0.3.2",
    "babel-polyfill": "^6.26.0",
    "deepmerge": "^4.2.2",
    "graphql": "^15.5.0",
    "graphql-request": "^3.4.0",
    "graphql-tag": "^2.11.0",
    "highlight.js": "^10.6.0",
    "jsonpointer": "^4.1.0",
    "jss-preset-default": "^10.7.1",
    "jwt-decode": "^3.1.2",
    "libphonenumber-js": "^1.9.23",
    "luxon": "^2.0.1",
    "password-validator": "^5.1.1",
    "path-to-regexp": "^6.2.0",
    "picklesdoc": "^1.1.1",
    "puppeteer": "^5.5.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/saasquatch/program-tools.git",
    "directory": "packages/mint-components"
  },
  "author": "SaaSquatch Team",
  "bugs": {
